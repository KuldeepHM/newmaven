pipeline {
   agent any 
stages {
    stage ('Compile Stage '){ 
steps {
     with maven ( maven : 'maven'){
     bat 'mvn clean compile'
     }
   }
}
    stage ('Testing Stage '){ 
steps {
      with maven ( maven : 'maven'){
      bat 'mvn test'
      }
    }
}
      stage ('Install Stage '){ 
steps {
      with maven ( maven : 'maven'){
      bat 'mvn install'
      }
     }
    }
   }
 }
